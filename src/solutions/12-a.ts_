interface Direction {x:number,y:number}

const directions = [
	{x:0,y:-1},
	{x:0,y:1},
	{x:-1,y:0},
	{x:1,y:0},
];

const dirStr:('up'|'down'|'left'|'right')[] = ['up','down','left','right'];

interface Plot extends Direction {
	isFenced: boolean
	p:string
	oFence: ('up'|'down'|'left'|'right')[]
}

type Fence = {
	area:number,
	perimeter:number,
	outer:number
}

const garden:Plot[][] = input.split('\n').map((row,y)=>row.split('').map((p,x)=>({p,x,y,isFenced:false,oFence:[]})));
const gardenHeight = input.split('\n').length;
const gardenWidth = input.split('\n')[0].length;

const fences:Fence[] = [];


const boundryCheck = (c:Direction) => c.x > -1 && c.x < gardenWidth && c.y > -1 && c.y < gardenHeight;

const sum = (a:Direction,b:Direction) => ({y:a.y+b.y,x:a.x+b.x}); 



garden.forEach(row=>row.forEach(plot=> {
	if (plot.isFenced){
		return;
	}
	console.log('-----');
	const fence:Fence = {area:1,perimeter:0,outer:0};

	const check = (d:Direction,p:Plot,i:number) => {
		const location = sum(d,p);
		if(!boundryCheck(location)){
			fence.perimeter++;
			fence.outer++;
			p.oFence.push(dirStr[i]);
			return;
		}
		const plot = garden[location.y][location.x];
		if (plot.p!==p.p){
			fence.perimeter++;
			return;
		}
		if (plot.isFenced) {
			return;
		}
		fence.area++;
		garden[location.y][location.x].isFenced = true;
		crawl(plot);
	};

	const crawl = (p:Plot) => {
		p.isFenced = true;
		directions.forEach((d,i)=>{
			check(d,p,i);
		});
	};

	crawl(plot);
	fences.push(fence);

}));

//console.debug(garden.flatMap(x=>x));
console.log(fences);
console.log(fences.reduce((acc,cur)=>acc+(cur.area*cur.perimeter),0));





