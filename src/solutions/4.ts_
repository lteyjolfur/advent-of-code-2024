const table:string[][] = input.split('\n').map(row => row.split(''));

let xCount = 0;

const word = ['X','M','A','S'];
//const word = ['S','A','M','X'];

// Boundry

const boundryRight = (i:number,j:number) => {
	if(j+3 > table[i].length){
		return true;
	}
	return false;
};

const boundryLeft = (i:number,j:number) => {
	if(j-3 < 0){
		return true;
	}
	return false;
};

const boundryDown = (i:number) => {
	if(i+3 >= table.length){
		return true;
	}
	return false;
};

const boundryUp = (i:number) => {
	if(i-3 < 0){
		return true;
	}
	return false;
};

// Search

const searchRight = (i:number,j:number) => {
	if(boundryRight(i,j)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i][j+index];		
	}) ? 1 : 0;
};
const searchRUp = (i:number,j:number) => {
	if(boundryRight(i,j)||boundryUp(i)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i-index][j+index];		
	}) ? 1 : 0;
};
const searchUp = (i:number,j:number) => {
	if(!boundryUp(i)){
		return word.every((c,index) => {
			//console.log(c,table[i+index][j],i+index);
			return c===table[i-index][j];		
		}) ? 1 : 0;
	}
	return 0;
};
const searchLup = (i:number,j:number) => {
	if(boundryLeft(i,j)||boundryUp(i)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i-index][j-index];		
	}) ? 1 : 0;
};
const searchLeft = (i:number,j:number) => {
	if(boundryLeft(i,j)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i][j-index];		
	}) ? 1 : 0;
};
const searchLDown = (i:number,j:number) => {
	if(boundryLeft(i,j)||boundryDown(i)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i+index][j-index];		
	}) ? 1 : 0;
};
const searchDown = (i:number,j:number) => {
	console.log('x found i=',i);
	if(!boundryDown(i)){
		return word.every((c,index) => {
			//console.log(c,table[i+index][j],i+index);
			return c===table[i+index][j];		
		}) ? 1 : 0;
	}
	return 0;
};
const searchRDown = (i:number,j:number) => {
	if(boundryRight(i,j)||boundryDown(i)){
		return 0;
	}
	return word.every((c,index) => {
		return c===table[i+index][j+index];		
	}) ? 1 : 0;
};

console.log(table.length);

table.forEach((row,i)=>{
	row.forEach((c,j)=>{
		if (c === word[0]){ //'X'
			xCount += searchRight(i,j);
			xCount += searchRUp(i,j);
			xCount += searchUp(i,j);
			xCount += searchLup(i,j);
			xCount += searchLeft(i,j);
			xCount += searchLDown(i,j);
			xCount += searchDown(i,j);
			xCount += searchRDown(i,j);
		}
	});

});

